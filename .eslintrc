{
    "plugins": ["react"],
    "parser": "babel-eslint",
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "env": {
        "browser": true,
        "node": true,
        "mocha": true,
        "es6": true
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "modules": true
        }
    },
    "settings": {
        "react": {
            "version": "16.8"
        }
    },
    "rules": {
        "react/prop-types": 1,
        "no-unused-vars": 1,
        "no-console": 1,
        "no-extra-boolean-cast": 0
    }
}
